#include <stdio.h>

int main() {
    int idade = 19;
    float altura = 1.70;
    float peso = 80.0;
    float imc = peso / (altura * altura);

    printf("Idade: %d anos\n", idade);
    printf("Altura: %.2f m\n", altura);
    printf("Peso: %.2f kg\n", peso);
    printf("IMC: %.2f\n", imc);

    return 0;
}
