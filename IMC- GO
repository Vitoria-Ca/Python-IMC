package main

import (
    "fmt"
)

func main() {
    idade := 19
    altura := 1.70
    peso := 80.0

    imc := peso / (altura * altura)

    fmt.Printf("Idade: %d anos\n", idade)
    fmt.Printf("Altura: %.2f m\n", altura)
    fmt.Printf("Peso: %.2f kg\n", peso)
    fmt.Printf("IMC: %.2f\n", imc)
}
